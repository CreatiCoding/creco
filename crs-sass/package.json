    "debug": "^3.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "http-errors": "^1.7.0",
    "morgan": "^1.9.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1"
  }
