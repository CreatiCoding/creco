{
  "name": "portfolio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set DEV=true && concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "deploy": "yarn build && set DEV=false && set PORT=3000 && nodemon ./bin/www",
    "server": "set PORT=3001 && nodemon ./bin/www",
    "client": "set PORT=3000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "http-errors": "^1.7.1",
    "morgan": "^1.9.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1"
  },
  "proxy": "http://localhost:3001/",
  "homepage": ""
}
