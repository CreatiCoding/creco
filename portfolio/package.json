{
  "name": "portfolio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEV=true concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "deploy": "yarn build && DEV=false PORT=3000 nodemon ./bin/www",
    "server": "PORT=3001 nodemon ./bin/www",
    "client": "PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass-dev": "sass --load-path=node_modules --watch src/scss:src/css",
    "sass-pro": "sass --load-path=node_modules src/scss:src/css"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "@material/top-app-bar": "^0.39.3",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "http-errors": "^1.7.1",
    "morgan": "^1.9.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1"
  },
  "proxy": "http://localhost:3001/",
  "homepage": ""
}
